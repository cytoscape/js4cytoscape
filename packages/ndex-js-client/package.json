{
  "name": "ndex-client",
  "version": "0.1.0",
  "description": "NDEx Javascript library",
  "main": "lib/ndexClient.js",
  "scripts": {
    "prepublish": "run-s build",
    "lint:js": "eslint ./src",
    "bundle:js": "webpack",
    "watch:js": "webpack --watch",
    "watch:sync-bundle": "browser-sync start --config browser-sync.config.js",
    "bundle": "run-s bundle:*",
    "build": "run-p bundle",
    "build-prod": "cross-env NODE_ENV=production run-s build",
    "clean": "rimraf build/*",
    "lint": "run-s lint:*",
    "watch": "run-p watch:*",
    "test": "mocha"
  },
  "keywords": [
    "webpack",
    "es6",
    "starter",
    "library",
    "universal",
    "umd",
    "commonjs"
  ],
  "author": "Jing Chen",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-env": "^1.6.0",
    "browser-sync": "^2.18.13",
    "canvas": "^2.5.0",
    "chai": "^4.1.2",
    "cross-env": "^5.0.0",
    "echo-cli": "^1.0.8",
    "eslint": "^5.0.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "jsdom": "15.2.1",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "nock": "^13.0.2",
    "npm-run-all": "^4.1.1",
    "rimraf": "^2.6.2",
    "sinon": "^7.1.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.12.2",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "node": ">=6.3.0"
  }
}
